    "def remove_newlines(series): \n",
    "    series = series.str.replace('\\n', ' ') \n",
    "    series = series.str.replace('\\\\n', ' ') \n",
    "    series = series.str.replace('  ', ' ') \n",
    "    series = series.str.replace('  ', ' ') \n",
