   "source": [
    "# Create the FAISS index\n",
    "d = vectors.shape[1]  # Dimensionality of the vectors\n",
    "index = faiss.IndexFlatL2(d)\n",
    "\n",
    "# Add vectors to the index\n",
    "index.add(vectors)"
   ]
